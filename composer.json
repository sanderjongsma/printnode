    "extra": {
        "laravel": {
            "providers": [
                "SanderJongsma\\PrintNode\\PrintNodeServiceProvider"
            ],
            "aliases": {
                "PrintNode": "SanderJongsma\\PrintNode\\Facades\\PrintNode"
