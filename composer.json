        }
    ],
    "homepage": "https://github.com/sanderjongsma/printnode",
    "keywords": ["Laravel", "PrintNode"],
    "require": {
        "illuminate/support": "~5",
        "printnode/printnode-php": "^2.0@RC"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
